{
	"name": "platzi-webpack",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "webpack --entry ./index.js --output ./bundle.js --mode development",
		"build:local": "webpack --mode production",
		"build:external": "webpack --config ./external/webpack.config.js",
		"build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
		"build:css": "webpack --config ./css-style-loader/webpack.config.js",
		"build:plugins": "webpack --config ./plugins/webpack.config.js",
		"build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --open",
		"build:babel": "webpack-dev-server --config ./babel-pokemon/webpack.config.js --open",
		"build:babel-dist": "webpack --config ./babel-pokemon/webpack.config.js --open",
		"build:react": "webpack-dev-server --config ./react/webpack.config.js --open",
		"build:react-dist": "webpack --config ./react/webpack.config.js --open",
		"build:loader": "webpack-dev-server --config ./url-loader/webpack.config.js --open",
		"build:loader-dist": "webpack --config ./url-loader/webpack.config.js --open",
		"build:prepro": "webpack-dev-server --config ./preprocesadores/webpack.config.js --open",
		"build:prepro-dist": "webpack --config ./preprocesadores/webpack.config.js --open",
		"build:prevent-duplication": "webpack-dev-server --config ./prevent-duplication-react/webpack.config.js --open",
		"build:prevent-duplication-dist": "webpack --config ./prevent-duplication-react/webpack.config.js --open",
		"build:dll": "webpack --config ./dll/webpack.dll.config.js --open",
		"build:prevent-duplication-fast-dist": "webpack --config ./dll/webpack.config.js --open",
		"build:dynamic:dll": "webpack --config ./dynamic-imports/webpack.dll.config.js --open",
		"build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js --open"
	},
	"keywords": [
		"webpack",
		"javascript"
	],
	"author": "Andres Campuzano <campuxano88@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.9.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"@babel/preset-react": "^7.9.4",
		"babel-loader": "^8.1.0",
		"css-loader": "^3.5.2",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^4.2.0",
		"less": "^3.11.1",
		"less-loader": "^6.0.0",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^7.0.0",
		"postcss-loader": "^3.0.0",
		"postcss-nested": "^4.2.1",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"url-loader": "^4.1.0",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3"
	}
}
